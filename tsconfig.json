{
  "compilerOptions": {
    "incremental": false,
    "lib": [
      "ES2023"
    ],
    "target": "ES2023",
    "module": "commonjs",
    "declaration": true,
    "sourceMap": true,
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitOverride": true,
    "composite": false,
    "rootDir": "src",
    "outDir": "build"
  },
  "exclude": [
    "build",
    "vitest.config.ts",
  ]
}